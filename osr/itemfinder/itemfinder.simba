// SHOULD ADD TO SRL
function TRSItemFinder.LoadItemImage(Item: String): TMufasaBitmap; override;
begin
  if not DirectoryExists(ITEM_FINDER_IMAGES_PATH) then
    CreateDirectory(ITEM_FINDER_IMAGES_PATH);

  Result := inherited(Item);
end;
