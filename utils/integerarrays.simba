{$IFNDEF WL_UTILS}
  {$I WaspLib/utils.simba}
{$ENDIF}

function TIntegerArray.Difference(Other: TIntegerArray): TIntegerArray;
var
  i: Int64;
begin
  for i in Self do
    if Other.IndexOf(i) = -1 then
      Result += i;

  for i in Other do
    if Self.IndexOf(i) = -1 then
      Result += i;

  Result.ClearDuplicates;
  Result := Result.Sorted();
end;
