{$DEFINE SRL_DISABLE_REMOTEINPUT}
{$I SRL-T/osr.simba}
{$I WaspLib/osr.simba}

var
  bioHashStr, password, newPassword: String;

begin
  Login.PlayerIndex := 0; //Player Index in order from what you have in credentials.simba
  bioHashStr  := StatsClient.GenerateBioHash(BioHash);
  password    := StatsClient.GeneratePassword('old_password');
  newPassword := StatsClient.GeneratePassword('new_password');

  StatsClient.UpdatePassword(bioHashStr, password, newPassword);
end.
